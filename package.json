{
    "name": "screencap",
    "longName": "screencap",
    "description": "screencap",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "format": "prettier --write \"./src/**/*.ts\" \"./src/**/*.tsx\"",
        "start": "concurrently  \"npm run build\" \"npm run dev\"",
        "tailwind": "tailwindcss -i src/styles/index.css -o dist/assets/index.css",
        "dev": "vite",
        "build": "tsc && vite build && npm run build:js",
        "build:pre": "esno scripts/prebuild.ts",
        "build:post": "esno scripts/postbuild.ts",
        "build:js": "vite build --config vite.config.content.ts",
        "preview": "vite preview"
    },
    "author": "Syket Das",
    "license": "ISC",
    "devDependencies": {
        "@types/chrome": "^0.0.240",
        "@types/fs-extra": "^11.0.1",
        "@types/node": "^20.14.13",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@types/webextension-polyfill": "^0.10.1",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "concurrently": "^8.2.0",
        "esno": "^0.16.3",
        "fs-extra": "^11.1.1",
        "postcss": "^8.4.25",
        "prettier": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6",
        "unplugin-auto-import": "^0.16.6",
        "vite": "^4.4.3",
        "vite-plugin-static-copy": "^0.16.0",
        "webext-bridge": "^6.0.1",
        "webextension-polyfill": "^0.10.0",
        "webextension-polyfill-ts": "^0.26.0"
    },
    "dependencies": {
        "@radix-ui/react-icons": "^1.3.0",
        "@radix-ui/react-menubar": "^1.1.1",
        "@radix-ui/react-slot": "^1.1.0",
        "@radix-ui/react-switch": "^1.1.0",
        "@radix-ui/react-tooltip": "^1.1.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "html2canvas": "^1.4.1",
        "react-icons": "^5.2.1",
        "react-rnd": "^10.4.11",
        "react-to-print": "^2.15.1",
        "react-zoom-pan-pinch": "^3.6.1",
        "tailwind-merge": "^2.4.0",
        "tailwindcss-animate": "^1.0.7"
    }
}
